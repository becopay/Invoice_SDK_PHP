{
    "name": "becopay/php-payment-gateway",
    "description": "Becopay payment gateway api php library",
    "keywords": ["payment"],
    "homepage": "https://github.com/becopay/php-payment-gateway/",
    "version": "0.0.1",
    "type": "library",
    "require": {
        "php": ">=5.3.0"
    },

    "license": "MIT",
    "authors": [
        {
            "name": "becopay team",
            "email": "io@becopay.com"
        }
    ],
    "support": {
      "issues": "https://github.com/becopay/php-payment-gateway/issues"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "becopay\\gateway\\": "src/"
        },
      "classmap": ["src/" ]
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^4"
    }
}
